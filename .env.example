TRAVELPAYOUTS_TIMEOUT=10000
TRAVELPAYOUTS_ENABLED=true
TRAVELPAYOUTS_AFFILIATE_ID=your_affiliate_id

SKYSCANNER_API_KEY=your_skyscanner_api_key
