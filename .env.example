SKYSCANNER_TIMEOUT=10000
SKYSCANNER_ENABLED=true

EXPEDIA_API_KEY=your_expedia_api_key
EXPEDIA_API_URL=https://expedia.com/api
EXPEDIA_TIMEOUT=10000
EXPEDIA_ENABLED=true
EXPEDIA_AFFILIATE_ID=your_affiliate_id

KIWI_API_KEY=your_kiwi_api_key
KIWI_API_URL=https://api.tequila.kiwi.com
KIWI_TIMEOUT=10000
